# Environment variables for Fastlane Android

# Google Play Console JSON key file path
# PLAY_STORE_JSON_KEY_PATH=./fastlane/google-play-key.json

# Git branch for releases
# GIT_BRANCH=main

# Keystore configuration for signing
# KEYSTORE_PATH=./keystores/release.keystore
# KEYSTORE_PASSWORD=your_keystore_password
# KEY_ALIAS=your_key_alias
# KEY_PASSWORD=your_key_password

# Fastlane settings
FASTLANE_SKIP_UPDATE_CHECK=1
FASTLANE_HIDE_CHANGELOG=1
