# Matchfile for certificate management

git_url ENV["MATCH_GIT_URL"] || "https://github.com/yourusername/certificates"

storage_mode("git")

type("appstore") # Can be appstore, adhoc, development, or enterprise

app_identifier(["com.mizanpro.app"])

username(ENV["APPLE_ID"] || "your-apple-id@example.com")

team_id(ENV["TEAM_ID"] || "YOUR_TEAM_ID")

# For more information about match, see:
# https://docs.fastlane.tools/actions/match/
