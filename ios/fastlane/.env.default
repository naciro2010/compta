# Environment variables for Fastlane iOS

# Apple ID for App Store Connect
# APPLE_ID=your-apple-id@example.com

# App Store Connect API Key (recommended over APPLE_ID)
# APP_STORE_CONNECT_API_KEY_KEY_ID=YOUR_KEY_ID
# APP_STORE_CONNECT_API_KEY_ISSUER_ID=YOUR_ISSUER_ID
# APP_STORE_CONNECT_API_KEY_KEY_FILEPATH=./AuthKey_XXXXXX.p8

# Team IDs
# TEAM_ID=YOUR_TEAM_ID
# ITC_TEAM_ID=YOUR_ITC_TEAM_ID

# Match (Certificate Management)
# MATCH_GIT_URL=https://github.com/yourusername/certificates
# MATCH_PASSWORD=your_match_password

# Git branch for releases
# GIT_BRANCH=main

# Fastlane settings
FASTLANE_SKIP_UPDATE_CHECK=1
FASTLANE_HIDE_CHANGELOG=1
